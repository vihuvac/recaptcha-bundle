{
    "name": "vihuvac/recaptcha-bundle",
    "description": "This bundle provides an easy reCAPTCHA form field integration into a Symfony Project",
    "keywords": ["Google", "Recaptcha", "Symfony Bundle", "Vihuvac"],
    "homepage": "https://github.com/vihuvac/recaptcha-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "VÃ­ctor Hugo Valle Castillo",
            "email": "victouk@gmail.com",
            "homepage": "https://vihuvac.com",
            "role": "Developer"
        },
        {
            "name": "Open Source Community",
            "homepage": "https://github.com/vihuvac/recaptcha-bundle/contributors"
        }
    ],
    "require": {
        "php": "^5.3 || ^7.0",
        "symfony/symfony": "^2.3 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Vihuvac\\Bundle\\RecaptchaBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vihuvac\\Bundle\\RecaptchaBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
